package typeCasting;

public class typeCasting {

	public static void main(String[] args) {

		int a = 5;
		int b = (int) 5.5; // put the type you want variable to become

		// several diff types of primitive variables
		byte aa;
		short bb;
		char c;
		int d;
		long e;
		float f;
		double g;

		// java is automatically cast UP but we must manually cast DOWN

	}

}
