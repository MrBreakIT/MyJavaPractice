package conditionalOperators;

public class ConditionalOperator {

	public static void main(String[] args) {

		int a = (7 > 3) ? 7 : 3;

		// same statement below
		if (7 > 3) {
			a = 7;
		} else {
			a = 3;
		}

		System.out.println(a);

		String b = "hello";
		double result = b.equals("hello") ? 0.5 : 0.25;

	}

}
